<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Travel" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ListViewStyle" parent="android:style/Widget.ListView">
        <item name="background">@color/black</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@color/white</item>
    </style>

    <style name="viewStyle">
        <item name="android:background">@color/white</item>
    </style>
</resources>